        data, address = receiver_sock.recvfrom(RECEIVE_SIZE)        # medzi prijatim paketov a zadavanim path na ulozenie
        calculated_sha = hashlib.sha256(reconstructed_message).hexdigest()
        received_sha = data.decode()
        if received_sha == calculated_sha:
            print("SHA256 hashes match.")
            print("Received: " + received_sha)
            print("Calculated: " + calculated_sha)
        else:
            print("Received: " + received_sha)
            print("Calculated: " + calculated_sha)
            print("SHA256 hashes do not match.")

-------------------------------------------cezar sifra----------------------------------------------
sender
            def cezarova_sifra(sprava, posun):
                abeceda = "abcdefghijklmnopqrstuvwxyz"
                posunuta_abeceda = abeceda[posun:] + abeceda[:posun]
                preklad = str.maketrans(abeceda + abeceda.upper(), posunuta_abeceda + posunuta_abeceda.upper())
                zasifrovana_sprava = sprava.translate(preklad)
                return zasifrovana_sprava

            user_message = cezarova_sifra(user_message, 3)          # na zaciatok send funkcie
            user_message = "_BEGIN_" + user_message + "_END_"

receiver
            def cezarova_desifra(sifrovana_sprava, posun):
                abeceda = "abcdefghijklmnopqrstuvwxyz"
                posunuta_abeceda = abeceda[-posun:] + abeceda[:-posun]
                preklad = str.maketrans(abeceda + abeceda.upper(), posunuta_abeceda + posunuta_abeceda.upper())
                desifrovana_sprava = sifrovana_sprava.translate(preklad)
                return desifrovana_sprava

            original_message = cezarova_desifra(original_message, 3)        # pred vypis rekonstruovanej spravy
            original_message = original_message[7:-5]

-------------------------------------------parne pakety----------------------------------------------

sender  packets = []
        total_packets = (len(encoded_message) + fragment_size - 1) // fragment_size
        sequence_number = 1
        seq_number_2 = 0
        print(total_packets)
        while sequence_number <= total_packets:
            start_idx = sequence_number * fragment_size
            end_idx = (sequence_number + 1) * fragment_size
            fragment_data = encoded_message[start_idx:end_idx]
            header = struct.pack('>HHH', len(fragment_data), int(total_packets//2), seq_number_2)
            if error and sequence_number + 1 == int(corrupted_packet):
                crc = "00".encode()
                real_crc = calculate_crc(header + fragment_data)
                real_packet = (header + fragment_data + real_crc)
            else:
                crc = calculate_crc(header + fragment_data)
            packet = (header + fragment_data + crc)
            packets.append(packet)
            sequence_number += 2
            seq_number_2 += 1
        print(packets)
        total_packets = int(total_packets // 2)


-------------------------------------------neparne pakety----------------------------------------------
sender
        packets = []
        total_packets = (len(encoded_message) + fragment_size - 1) // fragment_size
        sequence_number = 0
        seq_number_2 = 0
        print(total_packets)
        while sequence_number <= total_packets:
            start_idx = sequence_number * fragment_size
            end_idx = (sequence_number + 1) * fragment_size
            fragment_data = encoded_message[start_idx:end_idx]
            header = struct.pack('>HHH', len(fragment_data), int(total_packets // 2), seq_number_2)
            if error and sequence_number + 1 == int(corrupted_packet):
                crc = "00".encode()
                real_crc = calculate_crc(header + fragment_data)
                real_packet = (header + fragment_data + real_crc)
            else:
                crc = calculate_crc(header + fragment_data)
            packet = (header + fragment_data + crc)
            packets.append(packet)
            sequence_number += 2
            seq_number_2 += 1
        print(packets)
        total_packets = int(total_packets // 2)

