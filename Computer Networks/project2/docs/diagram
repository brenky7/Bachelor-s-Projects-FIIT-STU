        if nack:                            # na zaciatok v druhom try pri posielani paketov
            for i in range(5):
                sender_sock.send(packets[base])
                print("Sent packet", base + 1, "of", total_packets)
            nack = False

receiver
        if nack:                            # na zaciatok v try pri prijimani paketov
            for i in range(5):
                data, address = receiver_sock.recvfrom(RECEIVE_SIZE)
            nack = False

-------------------------------------------md5 hash----------------------------------------------
sender
        hash = hashlib.md5(user_message.encode(FORMAT)).hexdigest() # na zaciatok send message
        print("Hash of the message:", hash)
        user_message = user_message + "___" + hash



receiver
            received_message = original_message.split("___")[0]  # do vypisu prijatej spravy
            received_hash = original_message.split("___")[1]
            calulated_hash = hashlib.md5(received_message.encode(FORMAT)).hexdigest()
            print("Received message:", received_message)
            print("Received hash: ", received_hash)
            print("Calculated hash: ", calulated_hash)
            if received_hash == calulated_hash:
                print("Hashes match.")
            else:
                print("Hashes don't match.")